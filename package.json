{
  "name": "eol-blocker",
  "version": "1.0.11",
  "description": "GitHub action to detect and block pull requests with invalid line-endings",
  "main": "lib/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "test": "jest",
    "build": "tsc",
    "package": "ncc build --source-map",
    "all": "npm run format && npm run build && npm run test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoftgraph/eol-blocker.git"
  },
  "author": "jasonjoh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoftgraph/eol-blocker/issues"
  },
  "homepage": "https://github.com/microsoftgraph/eol-blocker#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@types/node-fetch": "^2.6.3",
    "@vercel/ncc": "^0.36.1",
    "minimatch": "^5.1.2",
    "node-fetch": "^2.6.11"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^14.0.0",
    "@octokit/webhooks-types": "^6.11.0",
    "@types/minimatch": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "endOfLine": "auto",
    "singleQuote": true
  }
}
