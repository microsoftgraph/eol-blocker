{
  "name": "eol-blocker",
  "version": "1.0.11",
  "description": "GitHub action to detect and block pull requests with invalid line-endings",
  "main": "lib/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "test": "jest",
    "build": "tsc",
    "package": "ncc build --source-map",
    "all": "npm run format && npm run build && npm run test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoftgraph/eol-blocker.git"
  },
  "author": "jasonjoh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoftgraph/eol-blocker/issues"
  },
  "homepage": "https://github.com/microsoftgraph/eol-blocker#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@types/node-fetch": "^2.6.2",
    "@vercel/ncc": "^0.34.0",
    "minimatch": "^5.1.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^14.0.0",
    "@octokit/webhooks-types": "^6.6.0",
    "@types/minimatch": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.2",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "prettier": {
    "endOfLine": "auto",
    "singleQuote": true
  }
}
