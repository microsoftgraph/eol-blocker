{
  "name": "eol-blocker",
  "version": "1.0.11",
  "description": "GitHub action to detect and block pull requests with invalid line-endings",
  "main": "lib/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "test": "jest",
    "build": "tsc",
    "package": "ncc build --source-map",
    "all": "npm run format && npm run build && npm run test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoftgraph/eol-blocker.git"
  },
  "author": "jasonjoh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoftgraph/eol-blocker/issues"
  },
  "homepage": "https://github.com/microsoftgraph/eol-blocker#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@types/node-fetch": "^2.6.2",
    "@vercel/ncc": "^0.36.0",
    "minimatch": "^5.1.2",
    "node-fetch": "^2.6.8"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^14.0.0",
    "@octokit/webhooks-types": "^6.9.0",
    "@types/minimatch": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.3.1",
    "nock": "^13.3.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "prettier": {
    "endOfLine": "auto",
    "singleQuote": true
  }
}
