{
  "name": "eol-blocker",
  "version": "1.0.14",
  "description": "GitHub action to detect and block pull requests with invalid line-endings",
  "main": "lib/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "test": "jest",
    "build": "tsc",
    "package": "ncc build --source-map",
    "all": "npm run format && npm run build && npm run test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoftgraph/eol-blocker.git"
  },
  "author": "jasonjoh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoftgraph/eol-blocker/issues"
  },
  "homepage": "https://github.com/microsoftgraph/eol-blocker#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@vercel/ncc": "^0.38.1",
    "minimatch": "^9.0.4",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^22.2.0",
    "@octokit/webhooks-types": "^7.5.1",
    "@types/minimatch": "^5.1.2",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "fetch-mock": "^9.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "prettier": {
    "endOfLine": "auto",
    "singleQuote": true
  }
}
